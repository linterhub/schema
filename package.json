{
  "author": "linterhub <hi@linterhub.com>",
  "name": "@linterhub/schema",
  "version": "0.0.0-development",
  "description": "JSON Schema definitions for linterhub",
  "homepage": "https://schema.linterhub.com",
  "license": "MIT",
  "contributors": [],
  "keywords": [
    "linterhub"
  ],
  "repository": "linterhub/schema",
  "bugs": "https://github.com/linterhub/schema/issues",
  "main": "index.js",
  "files": [
    "dist/*.json",
    "dist/*.d.ts",
    "index.d.ts",
    "README.md"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "tag": "latest",
    "access": "public"
  },
  "scripts": {
    "ajv": "./node_modules/.bin/ajv",
    "gulp": "./node_modules/gulp/bin/gulp.js",
    "eslint": "./node_modules/.bin/eslint",
    "conduct": "./node_modules/.bin/covgen conduct@linterhub.com ./.github/CODE_OF_CONDUCT.md",
    "doc": "mkdir -p docs/schema && jsonschema2md -o docs/schema -d build/ -e json -s build/schemaver.json -x docs/example",
    "test": "gulp test",
    "semantic-release": "semantic-release"
  },
  "engines": {},
  "husky": {
    "hooks": {
      "pre-commit": "gulp test",
      "post-commit": "gulp pull"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "@adobe/jsonschema2md": "^1.0.6",
    "@semantic-release/changelog": "^2.0.2",
    "@semantic-release/exec": "^2.2.4",
    "@semantic-release/git": "^5.0.0",
    "ajv-cli": "^3.0.0",
    "covgen": "^3.1.0",
    "del": "^3.0.0",
    "eslint": "^5.0.0",
    "eslint-config-google": "^0.9.1",
    "fancy-log": "^1.3.2",
    "gh-pages": "^1.2.0",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-data": "^1.3.1",
    "gulp-eslint": "^4.0.2",
    "gulp-exec": "^3.0.1",
    "gulp-git": "^2.7.0",
    "gulp-help-doc": "^1.1.1",
    "gulp-hub": "^0.8.0",
    "gulp-install": "^1.1.0",
    "gulp-json-format": "^2.0.0",
    "gulp-json-schema": "^1.0.0",
    "gulp-jsonschema-bundle": "0.0.3",
    "gulp-rename": "^1.4.0",
    "husky": "^1.0.0-rc.8",
    "js-yaml": "^3.12.0",
    "json-schema-to-typescript": "^5.5.0",
    "jsonschema": "^1.2.4",
    "markdownlint": "^0.10.0",
    "request": "^2.87.0",
    "semantic-release": "^15.5.4",
    "yargs": "^11.0.0"
  }
}
